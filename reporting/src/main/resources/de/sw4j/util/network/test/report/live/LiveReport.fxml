<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane minHeight="300.0" minWidth="400.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.sw4j.util.network.test.report.live.LiveReportController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="NEVER" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <HBox>
         <children>
            <Label minWidth="75.0" text="Connect">
               <HBox.margin>
                  <Insets top="2.0" />
               </HBox.margin>
               <padding>
                  <Insets left="6.0" right="3.0" top="5.0" />
               </padding>
            </Label>
            <TextField fx:id="connectField" disable="true" editable="false" prefWidth="75.0">
               <HBox.margin>
                  <Insets top="2.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox GridPane.columnIndex="1">
         <children>
            <Label minWidth="75.0" text="Server">
               <HBox.margin>
                  <Insets top="2.0" />
               </HBox.margin>
               <padding>
                  <Insets left="6.0" right="3.0" top="5.0" />
               </padding>
            </Label>
            <TextField fx:id="serverField" disable="true" editable="false" prefWidth="75.0">
               <HBox.margin>
                  <Insets top="2.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox GridPane.rowIndex="1">
         <children>
            <Label minWidth="75.0" text="Latency">
               <HBox.margin>
                  <Insets top="2.0" />
               </HBox.margin>
               <padding>
                  <Insets left="6.0" right="3.0" top="5.0" />
               </padding>
            </Label>
            <TextField fx:id="latencyField" disable="true" editable="false" prefWidth="75.0">
               <HBox.margin>
                  <Insets top="2.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox GridPane.columnIndex="1" GridPane.rowIndex="1">
         <children>
            <Label minWidth="75.0" text="Response">
               <HBox.margin>
                  <Insets top="2.0" />
               </HBox.margin>
               <padding>
                  <Insets left="6.0" right="3.0" top="5.0" />
               </padding>
            </Label>
            <TextField fx:id="responseField" disable="true" editable="false" prefWidth="75.0">
               <HBox.margin>
                  <Insets top="2.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <LineChart fx:id="connectTimeChart" createSymbols="false" legendSide="RIGHT" onScroll="#mouseScroll" GridPane.columnSpan="2" GridPane.rowIndex="2">
        <xAxis>
          <CategoryAxis fx:id="connectTimeCategory" label="Time" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis label="Connect Time (ms)" side="LEFT" />
        </yAxis>
      </LineChart>
      <LineChart fx:id="serverTimeChart" createSymbols="false" legendSide="RIGHT" onScroll="#mouseScroll" GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="2">
        <xAxis>
          <CategoryAxis fx:id="serverTimeCategory" label="Time" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis label="Server Time (ms)" side="LEFT" />
        </yAxis>
      </LineChart>
      <LineChart fx:id="latencyChart" createSymbols="false" legendSide="RIGHT" onScroll="#mouseScroll" GridPane.columnSpan="2" GridPane.rowIndex="3">
        <xAxis>
          <CategoryAxis fx:id="latencyCategory" label="Time" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis label="Latency (ms)" side="LEFT" />
        </yAxis>
      </LineChart>
      <LineChart fx:id="responseTimeChart" createSymbols="false" legendSide="RIGHT" onScroll="#mouseScroll" GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="3">
        <xAxis>
          <CategoryAxis fx:id="responseTimeCategory" label="Time" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis label="Response Time (ms)" side="LEFT" />
        </yAxis>
      </LineChart>
   </children>
</GridPane>
